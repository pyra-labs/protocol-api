{
  "name": "protocol-api",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx src/index.ts",
    "lint": "yarn biome lint --write",
    "test": "vitest run --pool=forks",
    "test:data": "vitest run --pool=forks src/test/data.test.ts",
    "test:user": "vitest run --pool=forks src/test/user.test.ts",
    "test:buildTransaction": "vitest run --pool=forks src/test/buildTransaction.test.ts",
    "test:tx": "vitest run --pool=forks src/test/buildTransaction.test.ts",
    "build": "tsc -p tsconfig.json",
    "start": "yarn build && node dist/index.js",
    "docker": "docker build --platform linux/amd64 -t protocol-api .",
    "docker:run": "docker run --platform linux/amd64 --env-file ./.env protocol-api"
  },
  "license": "ISC",
  "dependencies": {
    "@jup-ag/api": "^6.0.40",
    "@mrgnlabs/marginfi-client-v2": "^5.0.0",
    "@quartz-labs/logger": "1.1.0",
    "@quartz-labs/sdk": "0.13.4",
    "@solana/spl-token": "^0.4.9",
    "@solana/web3.js": "^1.98.0",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "helmet": "^8.0.0",
    "hpp": "^0.2.3",
    "node-cache": "^5.1.2",
    "webflow-api": "^3.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/hpp": "^0.2.6",
    "@types/node": "^22.7.8",
    "nodemon": "^3.1.7",
    "qs": "^6.13.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3",
    "vite-tsconfig-paths": "^5.1.3",
    "vitest": "^2.1.6"
  },
  "resolutions": {
    "rpc-websockets": "^9.0.4",
    "@solana/web3.js": "^1.98.0"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
